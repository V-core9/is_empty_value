  "description": "Simple async/sync checker functions for empty values. Can check if a value is empty or not. [ isEmpty(val) / notEmpty(val) ]",
  "main": "./dist/v_is_empty_value.cjs.js",
  "module": "./dist/v_is_empty_value.es.js",
  "jsnext:main": "./dist/v_is_empty_value.cjs.js",
  "scripts": {
    "types:export": "sh -c \"npx -p typescript tsc ./src/*.js --declaration --allowJs --emitDeclarationOnly --outDir types\"",
    "test": "jest",
    "build": "tsc --build",
    "build:rollup": "rollup -c ./rollup.config.js --bundleConfigAsCjs",
    "clean": "tsc --build --clean",
    "prepack": "npm run build && npm run test",
    "webpack_test": "npx webpack --config ./__tests__/webpack/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/V-core9/v_is_empty_value.git"
  },
  "keywords": [
    "is_not_empty_value",
    "is_empty_value",
    "empty",
    "checkEmpty",
    "testEmptyValue",
    "async/await",
    "isEmpty",
    "notEmpty",
    "v-core9"
  ],
